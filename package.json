{
  "name": "@opensite/hooks",
  "version": "0.0.1",
  "description": "TODO",
  "keywords": [
    "react",
    "hooks",
    "optimization",
    "utilities",
    "seo",
    "tree-shakeable"
  ],
  "homepage": "https://github.com/opensite-ai/opensite-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensite-ai/opensite-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/opensite-ai/opensite-hooks/issues"
  },
  "author": "OpenSite AI (https://opensite.ai)",
  "license": "BSD 3",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs",
      "types": "./dist/core/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm run build:lib && pnpm run build:umd",
    "build:lib": "tsc && node scripts/emit-cjs.js",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "bundle-analysis": "node scripts/analyze-bundle.js || true",
    "prepare": "husky",
    "prepack": "pnpm run build",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
  },
  "dependencies": {
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
